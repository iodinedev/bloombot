  "query": "\n        SELECT course_name, participant_role, graduate_role, SIMILARITY(LOWER(course_name), LOWER($1)) AS similarity_score\n        FROM course\n        WHERE SIMILARITY(LOWER(course_name), LOWER($1)) > $2 AND guild_id = $3\n        ORDER BY similarity_score DESC\n        LIMIT 1\n      ",
